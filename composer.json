{
    "name": "mpyw/eloquent-has-by-join",
    "description": "Convert has() and whereHas() constraints to join() ones for single-result relations.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "keywords": ["laravel", "eloquent", "has", "whereHas", "join", "laravel", "illuminate", "database", "query", "builder", "macro"],
    "autoload": {
        "psr-4": {
            "Mpyw\\EloquentHasByJoin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mpyw\\EloquentHasByJoin\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.1",
        "illuminate/database": "^5.6 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.6 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "php-coveralls/php-coveralls": "^2.1",
        "nilportugues/sql-query-formatter": "^1.2",
        "awobaz/compoships": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mpyw\\EloquentHasByJoin\\EloquentHasByJoinServiceProvider"
            ]
        }
    },
    "suggest": {
        "mpyw/eloquent-has-by-non-dependent-subquery": "Provides more versatile non-dependent subqueries"
    }
}
